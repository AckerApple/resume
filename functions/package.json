{
  "name": "functions",
  "scripts": {
    "build": "tsc --resolveJsonModule --allowJs",
    "watch": "tsc --resolveJsonModule --watch --allowJs",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "8"
  },
  "main": "lib/index.js",
  "dependencies": {
    "ack-host": "^0.8.0",
    "ack-node": "^1.5.0",
    "firebase-admin": "^7.0.0",
    "firebase-functions": "^2.3.0",
    "helmet": "^3.18.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6",
    "typescript": "^3.2.2"
  },
  "private": true
}
