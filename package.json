{
  "name": "resume",
  "version": "2.0.20",
  "description": "Resume of Acker Dawn Apple",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "stats": "webpack-bundle-analyzer dist/stats.json",
    "stats:html:dist": "npm-run-all stats:html:_dist stats:html:src",
    "stats:html:src": "webpack-bundle-analyzer dist/stats.json --mode static --report src/stats.html --no-open",
    "stats:html:_dist": "webpack-bundle-analyzer dist/stats.json --mode static --report dist/stats.html --no-open",
    "stats:html:cordova": "npm-run-all stats:html:ios stats:html:android",
    "stats:html:ios": "webpack-bundle-analyzer dist/stats.json --mode static --report cordova/ios/www/stats.html --no-open",
    "stats:html:android": "webpack-bundle-analyzer dist/stats.json --mode static --report cordova/android/www/stats.html --no-open",
    "start": "npm-run-all --parallel compile:changelog watch:templates watch:ng:hmr",
    "compile:changelog": "ack-pug-bundler ./changelog.md src/changelog.template.ts --outType ts --oneFile --skipRender",
    "watch": "npm run start",
    "watch:ng:hmr": "ng serve --open --configuration=hmr --host 0.0.0.0",
    "watch:templates": "npm run compile:templates -- --watch",
    "watch:ng": "ng serve --open",
    "build": "npm-run-all compile:changelog build:index compile:templates build:ng stats:html:dist build:cordova",
    "build:ios": "npm-run-all build:index:ios compile:templates build:ng:ios stats:html:ios build:cordova:ios",
    "build:ng": "ng build --prod --output-hashing=none --stats-json",
    "build:ng:cordova": "npm-run-all build:ng:ios build:ng:android",
    "build:ng:ios": "ng build ios --prod --output-hashing=none",
    "build:ng:android": "ng build android --prod --output-hashing=none",
    "build:cordova": "npm-run-all build:ng:cordova stats:html:cordova build:cordova:platforms",
    "build:cordova:platforms": "npm-run-all build:cordova:ios build:cordova:android",
    "build:cordova:ios": "cd ./cordova/ios && cordova build --prod --release",
    "build:cordova:android": "cd ./cordova/android && cordova build --prod --release",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:index": "npm-run-all build:index:prod build:index:dev build:index:cordova",
    "build:index:cordova": "npm-run-all build:index:ios build:index:android",
    "build:index:ios": "pug src/index.ios.pug --out src/",
    "build:index:android": "pug src/index.android.pug --out src/",
    "build:index:prod": "pug src/index.prod.pug --out src/",
    "build:index:dev": "pug src/index.pug --out src/",
    "compile:templates": "ack-pug-bundler ./src/app ./src/app --outFileExt template.ts --outType ts --oneToOne"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ackerapple/resume.git"
  },
  "keywords": [
    "resume",
    "acker",
    "dawn",
    "apple"
  ],
  "author": "Acker Dawn Apple",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ackerapple/resume/issues"
  },
  "homepage": "https://github.com/ackerapple/resume#readme",
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "^7.2.15",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "ack-angular": "^1.13.2",
    "ack-angular-fx": "^3.0.3",
    "ack-angular-webcam": "^1.9.3",
    "ack-css-boot": "^1.2.58",
    "agm-overlays": "^1.4.1",
    "cordova-plugin-iosrtc": "^4.0.2",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "js-sha512": "^0.8.0",
    "localforage": "^1.7.3",
    "ngx-page-scroll": "^6.0.0-beta.1",
    "ngx-page-scroll-core": "^6.0.0-beta.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "ack-pug-bundler": "^1.4.5",
    "ack-x": "^1.5.9",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "pug-cli": "^1.0.0-alpha6",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
